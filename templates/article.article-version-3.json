    "e8e3bf14-8030-48f7-87da-588e318367ea": {
      "type": "twocolumn",
      "settings": {
        "fc-heading": "1. Break It Up",
        "fc-content": "",
        "sc-heading": "",
        "sc-content": "Using a grinder, sharp scissors, or your fingers, break up about half a gram of weed (a couple of small buds, basically) into a neat pile. There’s really no such thing as too fine, but you definitely don’t want your weed to be too big. Aim for about the size of minced garlic.",
        "sc-content-mwidth": 640,
        "fc-width": 230,
        "sc-width": 640,
        "list-gap": 80,
        "padding_top": 40,
        "padding_bottom": 20,
        "padding_left": 30,
        "padding_right": 30,
